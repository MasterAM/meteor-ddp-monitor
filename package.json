{
  "name": "ddpmonitor",
  "version": "0.3.0",
  "description": "Monitor Meteor DDP Traffic in Chrome",
  "repository": {
    "type": "git",
    "url": "git://github.com/thebakeryio/meteor-ddp-monitor.git"
  },
  "keywords": [
    "meteor",
    "ddp"
  ],
  "scripts": {
    "start": "node server.js",
    "test": "eslint . && BABEL_JEST_STAGE=0 jest",
    "chrome": "npm run test && webpack"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "author": "The Bakery",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thebakeryio/meteor-ddp-monitor/issues"
  },
  "homepage": "https://chrome.google.com/webstore/detail/ddp-monitor/ippapidnnboiophakmmhkdlchoccbgje",
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "eslint": "^1.7.3",
    "eslint-plugin-react": "^3.6.3",
    "flux": "^2.1.1",
    "jest-cli": "^0.7.1",
    "react": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-json-tree": "^0.2.0",
    "underscore": "^1.8.3"
  }
}
